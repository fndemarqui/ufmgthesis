pkgs <- c("simsurv", "survsim", "rsurv", "PermAlgo")
d <- cranlogs::cran_downloads(pkgs, from  = "2024-06-06", to = lubridate::today()-1)
d |>
group_by(package) |>
summarise(
mean = mean(count),
sd = sd(count),
cv = 100*sd(count)/mean(count),
n = sum(count)
) |>
mutate(
`%` = 100*n/sum(n)
) |> arrange(desc(n))
